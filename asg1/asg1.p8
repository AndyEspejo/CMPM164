pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--initial  variables for player
bus = {}
bus.x = 20; bus.y = 20
bus.speed = 2
bus.strikes = 0

--list for deer obstacles
deer = {}
deer_x = 128


--cam variable
camx = 0
camy = 0

function _update()
	bus_input()
	move_cam()
	spawn_deer()
	if deer_x == 0 then
		del_deer()
		deer_x = 128
		spawn_deer()
	end
	
	
end

function _draw()
	cls()
	camera(camx,camy)
	map(0,0,0,0,128,32)
	spr(1,bus.x,bus.y)
	foreach(deer, draw_deer)
	print(deer_x,camx+50, camy+50)
end

--player input
function bus_input()
	if (btn(0)) bus.x-=bus.speed
	if (btn(1)) bus.x+=bus.speed
	if (btn(2)) bus.y-=bus.speed
	if (btn(3)) bus.y+=bus.speed
end

--spawn deer 
function spawn_deer()
 if #deer < 10 then
  deer_y = rnd(128) 
		add(deer,deer_y)
	end
end

--draw deer
function draw_deer(deer_y)
	spr(2,128,deer_y)
end

--delete deer when offscreen
function del_deer()
	deer = {}
end
--collision
function hit_deer()
	
	
	return false
end

--scrolls the camera
function move_cam()
	camx += 1
	deer_x -= 1
end


